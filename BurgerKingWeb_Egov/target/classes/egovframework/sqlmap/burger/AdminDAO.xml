<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ezen.burger.dao.AdminDAO">
	<select id="adminCheck" statementType="CALLABLE" parameterType="java.util.HashMap">
	{CALL adminCheck(#{ id } , 
		#{ref_cursor , mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=BurgerVO } ) }
	</select>
	
	<select id="getAllCountMem" statementType="CALLABLE" parameterType="java.util.HashMap">
	{CALL getAllCountMem(
		#{cnt , mode=OUT, jdbcType=INTEGER } ) }
	</select>
	
	<select id="listMember" statementType="CALLABLE" parameterType="java.util.HashMap">
	{CALL listMember(#{startNum}, #{endNum}, #{key} ,
		#{ref_cursor , mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=BurgerVO } ) }
	</select>
	
	<delete id="deleteMember" statementType="CALLABLE" parameterType="java.util.HashMap">
	{CALL deleteMember(#{mseq}) }
	</delete>
	
	<select id="getAllCountEvent" statementType="CALLABLE" parameterType="java.util.HashMap">
	{CALL getAllCountEvent(
		#{cnt , mode=OUT, jdbcType=INTEGER } ) }
	</select>
	
	<select id="listEvent" statementType="CALLABLE" parameterType="java.util.HashMap">
	{CALL listEvent(#{startNum}, #{endNum}, #{key} ,
		#{ref_cursor , mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=BurgerVO } ) }
	</select>
	
</mapper>